<div class="row">
	<div class="jumbotron">
	  <div class="container">
	    <h1>High Speed Rail America Club</h1>
	  </div>
	</div>
</div>

<div class="row" id="vid">
	<div class="col-md-12">
		<div class="container">
			<h1>High Speed Rail America Club</h1>
		</div>
		<video autoplay loop poster="https://s3.amazonaws.com/hsrac/testclip.mp4.jpg" id="bgvid">
			<source src="https://s3.amazonaws.com/hsrac/testclip.webm" type="video/webm">
			<source src="https://s3.amazonaws.com/hsrac/testclip.mp4" type="video/mp4">
		</video>
	</div>
</div>

<div class="container">
	<div class="row" id="thumbnails">
		<div class="col-xs-6 col-md-4">
			<h2><%= link_to "Research", posts_path, target: "_blank" %></h2>
			<img data-src="holder.js/200x200" class="img-thumbnail" alt="200x200" src="/assets/research.png" data-holder-rendered="true" style="width: 200px; height: 200px;">
		</div>
		<div class="col-xs-6 col-md-4">
			<h2>News</h2>
			<img data-src="holder.js/200x200" class="img-thumbnail" alt="200x200" src="/assets/news.png" data-holder-rendered="true" style="width: 200px; height: 200px;">
		</div>
		<div class="col-xs-6 col-md-4">
			<h2>About Us</h2>
			<img data-src="holder.js/200x200" class="img-thumbnail" alt="200x200" src="/assets/about.png" data-holder-rendered="true" style="width: 200px; height: 200px;">
		</div>
	</div>
</div>

<div class="container">
	<div class="row" id="feature-in">
		<div class="col-md-2">
			<h4>Featured in:</h4>
		</div>
		<div class="col-md-3" id="miaminewtimes">
			<img src="/assets/miaminewtimes.png" class="img-responsive" atl="miaminewtimes" >
		</div>
	</div>
</div>

<footer>
	<div class="container">
		<div class="row">
			<div class="col-md-6" id="connect">
				<h1>Connect</h1>
			</div>
		</div>
		<div class="row">
			<div class="col-md-1" id="twitter">
				<a href="https://twitter.com/HSRAClub" target="_blank">
					<img src="/assets/twitter.png" class="img-responsive" atl="twitter" >
				</a>
			</div>
			<div class="col-md-1" id ="facebook">
				<a href="https://www.facebook.com/groups/hsrac/" target="_blank">
					<img src="/assets/facebook.png" class="img-responsive" atl="facebook" >
				</a>
			</div>
			<div class="col-md-1" id ="youtube">
				<a href="https://www.youtube.com/channel/UCpiBD6csbMlsEt20d_CmRCA" target="_blank">
					<img src="/assets/youtube.png" class="img-responsive" atl="youtube" >
				</a>
			</div>
			<div class="col-md-3 col-md-offset-6" id ="member_login">
				<% if !user_signed_in? %>
					<h2><%= link_to "Member Login", user_session_path %></h2>
				<% end %>
			</div>
		</div>
		<div class="row">
